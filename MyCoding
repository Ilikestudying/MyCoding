phone = input("Phone:")
digits_maping = {
    "0": "Zero",
    "1": "One",
    "2": "Two",
    "3": "Three",
    "4": "Four",
    "5": "Five",
    "6": "Six",
    "7": "Seven",
    "8": "Eight",
    "9": "Nine",
    "10": "Ten"
}
output = ""
for ch in phone:
    output += digits_maping.get(ch, "!") + " "
print(output)
